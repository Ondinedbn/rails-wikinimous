<h2><%= @article.title %></h2>
<p><%= @article.content %></p>

<%= link_to 'Edit this article', edit_article_path(@article)%>
<%= link_to "Delete #{@article.title}",
            article_path(@article),
            method: :delete,
            data: { confirm: "Delete #{@article.title}?" } %>
<%= link_to 'Back to articles', articles_path%>
